<p>All students</p>

<% @students.each do |student| %>
  <p>First Name: <%= student.first_name %></p>
  <p>Last Name: <%= student.last_name %></p>
  <p>Age: <%= student.age %></p>
  <p>House: <%= student.house.name.capitalize%></p>
  <a href="students/<%= student.id %>">Info</a>

  <hr />

  <%end%>
